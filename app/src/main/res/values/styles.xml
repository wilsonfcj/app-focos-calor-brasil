<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="DialogAnimationUpDown">
        <item name="android:windowEnterAnimation">@anim/slide_up</item>
        <item name="android:windowExitAnimation">@anim/slide_down</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="StyledDialogs_DialogAnimationNormal">
        <item name="android:background">@drawable/bg_dialog</item>
        <item name="android:windowEnterAnimation">@anim/dialog_styled_slide_up_normal</item>
        <item name="android:windowExitAnimation">@anim/dialog_styled_slide_down_normal</item>
    </style>
</resources>
